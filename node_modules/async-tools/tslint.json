{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "quotemark": [
            true,
            "double"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ]
    },
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "array-type": [
            true,
            "generic"
        ],
        "no-console": [false],
        "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": []
}